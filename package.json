{
  "name": "apply-html",
  "version": "1.0.0-10",
  "description": "It's `.innerHTML = ''` for the modern world.",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && rollup -c",
    "test": "xo && nyc ava -v",
    "report": "nyc report -r text-lcov | coveralls",
    "precommit": "npm run build && npm run test",
    "prepublishOnly": "git diff-index --quiet HEAD"
  },
  "keywords": [
    "apply",
    "diff",
    "dom",
    "html",
    "isomorphic",
    "literal",
    "patch",
    "render",
    "tag",
    "template"
  ],
  "author": "Shannon Moeller <me@shannonmoeller> (http://shannonmoeller.com)",
  "homepage": "https://github.com/shannonmoeller/apply-html#readme",
  "repository": "shannonmoeller/apply-html",
  "license": "MIT",
  "main": "index.js",
  "module": "src/apply-html.js",
  "browser": "dist/apply-html.js",
  "files": [
    "index.js",
    "dist",
    "src"
  ],
  "dependencies": {
    "@std/esm": "^0.19.2",
    "nanomorph": "^5.1.3"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "coveralls": "^3.0.0",
    "husky": "^0.14.3",
    "nyc": "^11.4.1",
    "rollup": "^0.54.0",
    "xo": "^0.18.2"
  },
  "@std/esm": "cjs",
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "xo": {
    "envs": [
      "browser",
      "node"
    ]
  },
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  }
}
