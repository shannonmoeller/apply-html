{
  "name": "apply-html",
  "version": "1.0.0-16",
  "description": "It's `.innerHTML = ''` for the modern world.",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && rollup -c",
    "lint": "run-s lint-*",
    "lint-prettier": "prettier $npm_package_config_ignore --write '{src,test}/**/*.js'",
    "lint-eslint": "eslint $npm_package_config_ignore --fix '{src,test}/**/*.js'",
    "test": "nyc -r none run-s test-* && nyc report -r lcov -r text",
    "test-node": "rollup test/node.js -c rollup.test.js | node | tap-diff",
    "test-browser": "rollup test/browser.js -c rollup.test.js | run-headless | tap-diff",
    "report": "nyc report -r text-lcov | coveralls",
    "pretest": "run-s lint",
    "precommit": "run-s test build && git add .",
    "prepublishOnly": "git diff-index --quiet HEAD"
  },
  "keywords": [
    "apply",
    "bel",
    "diff",
    "dom",
    "html",
    "isomorphic",
    "literal",
    "morph",
    "patch",
    "render",
    "tag",
    "template"
  ],
  "author": "Shannon Moeller <me@shannonmoeller> (http://shannonmoeller.com)",
  "repository": "shannonmoeller/apply-html",
  "license": "MIT",
  "main": "dist/apply-html.cjs.js",
  "module": "dist/apply-html.es.js",
  "browser": "dist/apply-html.umd.js",
  "files": [
    "dist",
    "*.md"
  ],
  "dependencies": {
    "nanomorph": "github:shannonmoeller/nanomorph#cc1b2b6"
  },
  "devDependencies": {
    "blue-tape": "^1.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.16.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-whim": "^3.0.0",
    "husky": "^0.14.3",
    "lodash.camelcase": "^4.3.0",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.4.1",
    "prettier": "^1.10.2",
    "rollup": "^0.54.0",
    "rollup-plugin-istanbul": "^2.0.0",
    "rollup-preset-node": "^1.0.0-2",
    "run-headless": "^2.0.1",
    "tap-diff": "^0.1.1"
  },
  "prettier": {
    "single-quote": true,
    "trailing-comma": "es5"
  },
  "eslintConfig": {
    "env": {
      "es6": true
    },
    "extends": [
      "eslint-config-whim",
      "eslint-config-prettier"
    ]
  },
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  }
}
